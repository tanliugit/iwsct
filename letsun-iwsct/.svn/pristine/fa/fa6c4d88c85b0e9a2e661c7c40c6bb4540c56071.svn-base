<!DOCTYPE html>
<html>
<head>
	<title>编辑</title>
	<th:block th:include="/common/layout/header" />
</head>
<body>
	<form class="layui-form layui-form-pane" action="edit">
		<input type="hidden" name="id" th:value="${obj.id}" />
		<div class="layui-form-item">
			<label class="layui-form-label">RESUMEID</label>
			<div class="layui-input-block">
				<input type="text" name="resumeid" th:value="${obj.resumeid}" class="layui-input" lay-verify="required" placeholder="请输入RESUMEID"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">CORPID</label>
			<div class="layui-input-block">
				<input type="text" name="corpid" th:value="${obj.corpid}" class="layui-input"  placeholder="请输入CORPID"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">职位id</label>
			<div class="layui-input-block">
				<input type="text" name="positionid" th:value="${obj.positionid}" class="layui-input"  placeholder="请输入职位id"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">手机号</label>
			<div class="layui-input-block">
				<input type="text" name="mobile" th:value="${obj.mobile}" class="layui-input"  placeholder="请输入手机号"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">应聘人</label>
			<div class="layui-input-block">
				<input type="text" name="candidate" th:value="${obj.candidate}" class="layui-input"  placeholder="请输入应聘人"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">籍贯</label>
			<div class="layui-input-block">
				<input type="text" name="hail" th:value="${obj.hail}" class="layui-input"  placeholder="请输入籍贯"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">身份证号</label>
			<div class="layui-input-block">
				<input type="text" name="idnumber" th:value="${obj.idnumber}" class="layui-input"  placeholder="请输入身份证号"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">性别</label>
			<div class="layui-input-block">
				<input type="text" name="sex" th:value="${obj.sex}" class="layui-input"  placeholder="请输入性别"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">年龄</label>
			<div class="layui-input-block">
				<input type="text" name="age" th:value="${obj.age}" class="layui-input"  placeholder="请输入年龄"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">身高</label>
			<div class="layui-input-block">
				<input type="text" name="height" th:value="${obj.height}" class="layui-input"  placeholder="请输入身高"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">最高学历</label>
			<div class="layui-input-block">
				<input type="text" name="education" th:value="${obj.education}" class="layui-input"  placeholder="请输入最高学历"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">学校</label>
			<div class="layui-input-block">
				<input type="text" name="school" th:value="${obj.school}" class="layui-input"  placeholder="请输入学校"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">专业</label>
			<div class="layui-input-block">
				<input type="text" name="specialty" th:value="${obj.specialty}" class="layui-input"  placeholder="请输入专业"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">个人说明</label>
			<div class="layui-input-block">
				<input type="text" name="content" th:value="${obj.content}" class="layui-input"  placeholder="请输入个人说明"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">备注</label>
			<div class="layui-input-block">
				<input type="text" name="remarks" th:value="${obj.remarks}" class="layui-input"  placeholder="请输入备注"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">提交进度：1:已提交，2：审核中，3:（其他）</label>
			<div class="layui-input-block">
				<input type="text" name="schedule" th:value="${obj.schedule}" class="layui-input"  placeholder="请输入提交进度：1:已提交，2：审核中，3:（其他）"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">1:正常，2：禁用，0:删除（其他）</label>
			<div class="layui-input-block">
				<input type="text" name="status" th:value="${obj.status}" class="layui-input"  placeholder="请输入1:正常，2：禁用，0:删除（其他）"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">CREATECORP</label>
			<div class="layui-input-block">
				<input type="text" name="createcorp" th:value="${obj.createcorp}" class="layui-input"  placeholder="请输入CREATECORP"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">AREAID</label>
			<div class="layui-input-block">
				<input type="text" name="areaid" th:value="${obj.areaid}" class="layui-input"  placeholder="请输入AREAID"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">ENGLEVEL</label>
			<div class="layui-input-block">
				<input type="text" name="englevel" th:value="${obj.englevel}" class="layui-input"  placeholder="请输入ENGLEVEL"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">毕业时间</label>
			<div class="layui-input-block">
				<input type="text" name="graduation" th:value="${obj.graduation}" class="layui-input"  placeholder="请输入毕业时间"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">目前工作公司</label>
			<div class="layui-input-block">
				<input type="text" name="currentcompany" th:value="${obj.currentcompany}" class="layui-input"  placeholder="请输入目前工作公司"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">目前工作岗位</label>
			<div class="layui-input-block">
				<input type="text" name="currentjob" th:value="${obj.currentjob}" class="layui-input"  placeholder="请输入目前工作岗位"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">工作年限</label>
			<div class="layui-input-block">
				<input type="text" name="worklonger" th:value="${obj.worklonger}" class="layui-input"  placeholder="请输入工作年限"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">期望薪资</label>
			<div class="layui-input-block">
				<input type="text" name="expectations" th:value="${obj.expectations}" class="layui-input"  placeholder="请输入期望薪资"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">年级</label>
			<div class="layui-input-block">
				<input type="text" name="grade" th:value="${obj.grade}" class="layui-input"  placeholder="请输入年级"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">邮箱</label>
			<div class="layui-input-block">
				<input type="text" name="email" th:value="${obj.email}" class="layui-input"  placeholder="请输入邮箱"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">活动ID</label>
			<div class="layui-input-block">
				<input type="text" name="hractivityid" th:value="${obj.hractivityid}" class="layui-input"  placeholder="请输入活动ID"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">关联的粉丝ID</label>
			<div class="layui-input-block">
				<input type="text" name="fansid" th:value="${obj.fansid}" class="layui-input"  placeholder="请输入关联的粉丝ID"/>
			</div>
		</div>
		<div class="layui-form-item">
			<div class="layui-input-block">
			     <button class="layui-btn" lay-submit="" lay-filter="layui-form-submit-btn">提交</button>
			     <button id="layui-form-cancel-btn" type="button" class="layui-btn layui-btn-primary">取消</button>
			 </div>
		</div>
	</form>
	<th:block th:include="/common/layout/footJs" />
</body>
</html>