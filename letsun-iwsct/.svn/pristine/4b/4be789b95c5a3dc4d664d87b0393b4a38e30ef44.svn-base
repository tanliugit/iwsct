<!DOCTYPE html>
<html>
<head>
	<title>新增</title>
	<th:block th:include="/common/layout/header" />
</head>
<body>
	<form class="layui-form layui-form-pane" action="add">
		<div class="layui-form-item">
			<label class="layui-form-label">微刊URL</label>
			<div class="layui-input-block">
				<input type="text" name="url" class="layui-input" lay-verify="required" placeholder="请输入微刊URL"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">粉丝ID</label>
			<div class="layui-input-block">
				<input type="text" name="fansid" class="layui-input" lay-verify="required" placeholder="请输入粉丝ID"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">CORPID</label>
			<div class="layui-input-block">
				<input type="text" name="corpid" class="layui-input" lay-verify="required" placeholder="请输入CORPID"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">ORGID</label>
			<div class="layui-input-block">
				<input type="text" name="orgid" class="layui-input" lay-verify="required" placeholder="请输入ORGID"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">统计类型：1:URL，2：点击，3:（其他）</label>
			<div class="layui-input-block">
				<input type="text" name="type" class="layui-input" lay-verify="required" placeholder="请输入统计类型：1:URL，2：点击，3:（其他）"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">访问IP</label>
			<div class="layui-input-block">
				<input type="text" name="visitip" class="layui-input" lay-verify="required" placeholder="请输入访问IP"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">访问次数</label>
			<div class="layui-input-block">
				<input type="text" name="visitsum" class="layui-input" lay-verify="required" placeholder="请输入访问次数"/>
			</div>
		</div>
		<div class="layui-form-item">
			<label class="layui-form-label">访问时间</label>
			<div class="layui-input-block">
				<input type="text" name="visittime" class="layui-input" lay-verify="required" placeholder="请输入访问时间"/>
			</div>
		</div>
		<div class="layui-form-item">
			<div class="layui-input-block">
			     <button class="layui-btn" lay-submit="" lay-filter="layui-form-submit-btn">提交</button>
			     <button id="layui-form-cancel-btn" type="button" class="layui-btn layui-btn-primary">取消</button>
			 </div>
		</div>
	</form>
	<th:block th:include="/common/layout/footJs" />
</body>
</html>